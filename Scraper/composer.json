{
    "name": "dealer-pro/scraper",
    "description": "Web scraper for dealer-pro application",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.9",
        "vlucas/phpdotenv": "^5.6",
        "symfony/dom-crawler": "^6.4",
        "symfony/css-selector": "^6.4",
        "monolog/monolog": "^3.9",
        "php-webdriver/webdriver": "^1.15"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
